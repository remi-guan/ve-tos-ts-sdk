{
  "name": "ve-tos-ts-sdk",
  "version": "1.1.0",
  "description": "Modern TypeScript SDK for Volcengine TOS (Object Storage) that works in any JavaScript runtime",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build && bun test",
    "lint": "bun x tsc --noEmit"
  },
  "keywords": [
    "volcengine",
    "tos",
    "object-storage",
    "s3",
    "cloudflare-workers",
    "deno",
    "bun",
    "edge",
    "typescript",
    "sdk"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/remi-guan/ve-tos-ts-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/remi-guan/ve-tos-ts-sdk/issues"
  },
  "homepage": "https://github.com/remi-guan/ve-tos-ts-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.0"
  }
}
